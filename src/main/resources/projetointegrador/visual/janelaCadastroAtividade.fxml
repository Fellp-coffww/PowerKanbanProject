         <font>
            <Font size="16.0" />
         </font>
      </Text>
      <Text layoutX="35.0" layoutY="205.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Data de inicio:">
         <font>
            <Font size="16.0" />
         </font>
      </Text>
      <TextField fx:id="tDatainicioA" layoutX="35.0" layoutY="211.0" promptText="dd/MM/yyyy" />
      <Text layoutX="35.0" layoutY="78.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Cadastro Atividade:">
         <font>
            <Font size="50.0" />
         </font>
